                        ShowNoti('Thông báo', 'Đăng nhập thành công', "success");
                        window.location.href = 'http://softdemo:8015/www';
                    }
                    if (result == '2') {
                        ShowNoti('Lỗi', 'Tài khoản của quý khách chưa xác thực. Vui lòng kiểm tra Email để xác thực', "error");
