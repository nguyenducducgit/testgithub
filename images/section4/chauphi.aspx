<script type="text/javascript">
    function LoginAccount() {
        Email = $("#EmailLogin").val();
        Password = $("#PasswordLogin").val();

        if (Email == '' || Password == '') {
            ShowNoti('Lỗi', 'Quý khách vui lòng nhập Email và mật khẩu', "error");
        }
        else {
            // Tương tác với server
            $.ajax({
                url: '/AccountCustomer/LoginAccount',
                data: { Email: Email, Password: Password },
                beforeSend: function () {
                    $(".divLoading").addClass("loading");
                },
                success: function (result) {
                    $(".divLoading").removeClass("loading");

                    if (result == '1') {
                        ShowNoti('Thông báo', 'Đăng nhập thành công', "success");
                        window.location.href = 'http://softdemo:8015/www';
                    }
                    if (result == '2') {
                        ShowNoti('Lỗi', 'Tài khoản của quý khách chưa xác thực. Vui lòng kiểm tra Email để xác thực', "error");
                    }
                    if (result == '0') {
                        ShowNoti('Lỗi', 'Đăng nhập không thành công', "error");
                    }

                    $("#EmailLogin").val('');
                    $("#PasswordLogin").val('');
                },
                error: function (message) {
                    $(".divLoading").removeClass("loading");

                    ShowNoti('Lỗi', message, "error");
                }
            });
        }
    }
