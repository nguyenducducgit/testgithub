        else {
            // Tương tác với server
            $.ajax({
                url: "/AccountCustomer/RegisterAccount",
                data: { Email: Email, Password: Password, Fullname: Fullname },
                success: function (result) {
                    if (result == '1') {

                        alert('Tạo mới tài khoản thành công. Quý khách cần kiểm tra email để kích hoạt tài khoản');
