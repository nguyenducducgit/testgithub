                    if (result == '0') {
                        alert('Tạo mới tài khoản thất bại.');
                    }

                    $("#EmailRegister").val('');
                    $("#PasswordRegister").val('');
                    $("#FullnameRegister").val('');
                }
            });
        }
    }
