    $(document).ready(function () {
        loadDeparture('1', 'departureIDFilter', '0');
        loadDeparture('1', 'departureIDFilterHeader', '0');
        loadPriceOption('1', 'priceIDFilter', '0');
        loadPriceOption('1', 'priceIDFilterHeader', '0');

        var date_from = $('input[name="dateFrom"]'); //our date input has the name "date"
        var container = $('.bootstrap-iso form').length > 0 ? $('.bootstrap-iso form').parent() : "body";
        date_from.datepicker({
            format: 'dd/mm/yyyy',
            container: container,
            todayHighlight: true,
            autoclose: true
        }).on('changeDate', function (ev) {
            if (dateFrom.localeCompare(ev.format())) {
                dateFrom = '' + ev.format();
                filterDate('DepartureDate', dateFrom);
            } else {
                dateFrom = '' + ev.format();
                console.log(dateFrom);
            }
        });

        var date_to = $('input[name="dateTo"]'); //our date input has the name "date"
        date_to.datepicker({
            format: 'dd/mm/yyyy',
            container: container,
            todayHighlight: true,
            autoclose: true
        }).on('changeDate', function (ev) {
            if (dateTo.localeCompare(ev.format())) {
                dateTo = '' + ev.format();
                filterDate('DepartureDateTo', dateTo);
            } else {
                dateTo = '' + ev.format();
                console.log(dateTo);
            }
        });
    });

    function filterDate(name, val) {
        //var parameterURL = $(this).attr('data-url');
        console.log('filterDate');
        var urlPath = changeURLParameter(name, val);
        document.location.href = urlPath;
    }
    function loadDeparture(type, id, valSelected) {
        // Tương tác với server
