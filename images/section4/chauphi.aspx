<script type="text/javascript">
    var dateFrom = '21/08/2020';
    var dateTo = '21/09/2020';

    $(".click-sort").click(function(event) {
        event.preventDefault();
        $(".show-ads").toggle();
        if ($(".mgbs").hasClass("mg-bs")) {
            $(".mgbs").removeClass("mg-bs");
        } else {
            $(".mgbs").addClass("mg-bs");
        }
        if ($(".click-sort .fas").hasClass("fa-arrow-alt-circle-down")) {
            $(".click-sort .fas").removeClass("fa-arrow-alt-circle-down").addClass("fa-arrow-alt-circle-up");
        } else {
            $(".click-sort .fas").removeClass("fa-arrow-alt-circle-up").addClass("fa-arrow-alt-circle-down");
