                    if (result == '0') {
                        ShowNoti('Lỗi', 'Đăng nhập không thành công', "error");
                    }

                    $("#EmailLogin").val('');
                    $("#PasswordLogin").val('');
                },
                error: function (message) {
                    $(".divLoading").removeClass("loading");

                    ShowNoti('Lỗi', message, "error");
                }
            });
        }
    }
