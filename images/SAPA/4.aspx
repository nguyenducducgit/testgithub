        var _listjson = '[{"Name":"Sapa","Lat":"22.338005","Long":"103.843036","Desription":"Sapa","Link":"https://travel.com.vn/mien-bac/tour-sapa.aspx"},{"Name":"Núi Fansipan","Lat":"22.303809","Long":"103.774828","Desription":"Núi Fansipan","Link":"https://travel.com.vn/mien-bac/tour-fan-si-pan.aspx"}]';
        var obj = $.parseJSON(_listjson);
        $.each(obj, function (key, val) {
            //alert(val.Name);
            var myLatLng = new google.maps.LatLng(parseFloat(val.Lat), parseFloat(val.Long));
            marker = new google.maps.Marker({
                map: map,
                //animation: google.maps.Animation.BOUNCE,
